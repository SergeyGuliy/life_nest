{
  "name": "life_nest",
  "private": true,
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "prepare": "husky install",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "starDb": "docker-compose up",
    "deleteTable": "docker exec -it life_postgres psql -U admin -d postgres -c \"DROP DATABASE users;\"",
    "createTable": "docker exec -it life_postgres psql -U admin -d postgres -c \"CREATE DATABASE users;\"",
    "cleanTable": "npm run deleteTable && npm run createTable"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@nestjs/common": "7.6.15",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "7.5.1",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/mongoose": "^7.2.4",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.5.1",
    "@nestjs/platform-socket.io": "^7.6.14",
    "@nestjs/typeorm": "^7.1.5",
    "@nestjs/websockets": "7.5.1",
    "@ntegral/nestjs-sentry": "^3.0.3",
    "@sentry/node": "^6.11.0",
    "@types/hapi__joi": "^17.1.6",
    "@types/socket.io": "^2.1.13",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "csurf": "^1.11.0",
    "dotenv": "^8.2.0",
    "easyimage": "^3.1.1",
    "helmet": "^4.4.1",
    "life_shared": "file:../life_shared",
    "lodash": "^4.17.21",
    "mathjs": "^11.3.2",
    "moment": "^2.29.4",
    "mongoose": "^6.5.3",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.5.1",
    "phone": "^2.4.21",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.6",
    "socket.io": "^4.5.4",
    "socket.io-redis": "^6.1.0",
    "throttle-debounce": "^3.0.1",
    "typeorm": "^0.2.31",
    "uuid": "^8.3.2",
    "ws": "latest"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.1",
    "@nestjs/schematics": "^7.1.3",
    "@nestjs/testing": "^7.5.1",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.15",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.6",
    "@types/passport-jwt": "^3.0.5",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.10",
    "@types/ws": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^8.0.1",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.1.2",
    "supertest": "^6.0.0",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.8",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.8.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
